{
  "name": "expo-and-typescript",
  "version": "0.0.0",
  "description": "React Native using Expo and TypeScript",
  "author": {
    "email": "jan@aagaard.net",
    "name": "Jan Aagaard"
  },
  "private": false,
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "check-types": "tsc --noEmit",
    "ios": "expo start --ios",
    "lint": "tslint --project tsconfig.json",
    "prettify": "prettier --write '{./,./src/**/}*.{js,json,ts,tsx}'",
    "start": "expo start",
    "validate": "yarn run lint && yarn run check-types"
  },
  "dependencies": {
    "expo": "32.0.6",
    "expo-core": "3.0.1",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-navigation": "3.3.2",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@types/expo": "32.0.5",
    "@types/expo__vector-icons": "9.0.1",
    "@types/fbemitter": "2.0.32",
    "@types/react": "16.8.6",
    "@types/react-native": "0.57.38",
    "@types/react-navigation": "3.0.4",
    "prettier": "1.16.4",
    "tslint": "5.13.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3333"
  }
}
